      //   for(int j=1; j<=n-i; j++) {
      //     System.out.print(" ");
      //   }
        
      //   // for star 
      //   for(int j=1; j<=i; j++) {
      //     System.out.print("* ");
      //   }
      //   System.out.println();
      // }
      
      
      // 18] Reverse Number Triangle pattern
      // 1 2 3 4 
      //  2 3 4 
      //   3 4 
      //    4 
      
      // int n=4;
      
      // // outer loop 
      // for(int i=1; i<=n; i++) {
        
