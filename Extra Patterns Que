      //   System.out.println();
      // }
      
      
      // // 19] Mirror image Triangle pattern
      // // 1 2 3 4 
      // //  2 3 4 
      // //   3 4 
      // //    4 
      // //   3 4 
      // //  2 3 4 
      // // 1 2 3 4 
      
      // int n=4;
      
      // // 1st part outer loop 
      // for(int i=1; i<=n; i++) {
      //   // inner loop 
      //   // for spaces 
      //   for (int j=1; j<=i-1; j++) {
